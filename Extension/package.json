{
	"name": "smalljs",
	"displayName": "SmallJS",
	"description": "SmallJS Smalltalk language support",
	"publisher": "RichardRonteltap",
	"version": "1.0.1",
	"icon": "SmallJS.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/Small-JS/SmallJS/tree/main/Extension"
	},
	 "engines": {
		"vscode": "^1.83.1"
	},
	"categories": [
		"Programming Languages"
	],
	"keywords": [
		"smalltalk"
	],
	"contributes": {
		"languages": [
			{
				"id": "smalljs",
				"aliases": [
					"SmallJS",
					"smalljs"
				],
				"extensions": [
					".st"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "smalljs",
				"scopeName": "source.smalljs",
				"path": "./syntaxes/smalljs.tmLanguage.json"
			}
		],
		"breakpoints": [
			{
				"language": "smalljs"
			}
		]
	}
}